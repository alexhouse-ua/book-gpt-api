{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/__init__",
      "dest": "functions/api/__init__.func"
    },
    {
      "src": "/api/add_book",
      "dest": "functions/api/add_book.func"
    },
    {
      "src": "/api/check_library_holds",
      "dest": "functions/api/check_library_holds.func"
    },
    {
      "src": "/api/delete_book",
      "dest": "functions/api/delete_book.func"
    },
    {
      "src": "/api/fetch_books",
      "dest": "functions/api/fetch_books.func"
    },
    {
      "src": "/api/ping",
      "dest": "functions/api/ping.func"
    },
    {
      "src": "/api/search_books",
      "dest": "functions/api/search_books.func"
    },
    {
      "src": "/api/sync_rss",
      "dest": "functions/api/sync_rss.func"
    },
    {
      "src": "/api/update_book",
      "dest": "functions/api/update_book.func"
    },
    {
      "src": "/internal/(.*)",
      "dest": "functions/api/maintenance/[task].func"
    }
  ]
}